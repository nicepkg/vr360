{
  "$schema": "https://turborepo.org/schema.json",
  "baseBranch": "origin/master",
  "pipeline": {
    "@nicepkg/vr360-core#build": {
      "dependsOn": ["@nicepkg/vr360-shared#build"],
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "@nicepkg/vr360-ui#build": {
      "dependsOn": ["@nicepkg/vr360-core#build"],
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "@nicepkg/vr360-ui-vue3#build": {
      "dependsOn": ["@nicepkg/vr360-ui#build"],
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "@nicepkg/vr360-ui-react#build": {
      "dependsOn": ["@nicepkg/vr360-ui#build"],
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "doc#build": {
      "dependsOn": ["^build"],
      "inputs": ["./**"],
      "outputs": ["./.vuepress/dist/**"]
    },
    "build": {
      "dependsOn": ["^build"],
      "inputs": ["src/**", "scripts/**", "types/**"],
      "outputs": ["dist/**"]
    },
    "test": {
      "dependsOn": ["build"],
      "outputs": []
    },
    "dev": {
      "cache": false
    }
  },
  "globalDependencies": ["tsconfig-base.json"]
}
