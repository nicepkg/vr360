  "private": true,
  "scripts": {
    "build": "stencil build --docs --legacy",
    "build:watch": "cross-env WATCH=true pnpm build",
    "clean": "rimraf ./dist/**/*",
    "dev": "cross-env NODE_ENV=development stencil build --docs --watch --legacy",
    "test": "stencil test --e2e --ci",
    "test:e2e": "stencil test --e2e",
    "test:e2e-update": "stencil test --e2e -u ./",
    "test:e2e-watch": "stencil test --e2e --watch",
    "test:spec": "stencil test --spec",
    "test:spec-watch": "stencil test --spec --watch",
    "type-check": "tsc --noEmit"
  },
  "main": "./dist/index.cjs.js",
  "module": "./dist/index.js",
  "es2015": "./dist/esm/index.js",
  "es2017": "./dist/esm/index.js",
  "jsnext:main": "./dist/esm/index.js",
  "collection": "./dist/collection/collection-manifest.json",
  "collection:main": "./dist/collection/index.js",
  "types": "./dist/types/interface.d.ts",
  "unpkg": "./dist/vr360-ui/vr360-ui.esm.js",
  "jsdelivr": "./dist/vr360-ui/vr360-ui.esm.js",
  "exports": {
    ".": {
      "require": "./dist/index.cjs.js",
      "import": "./dist/index.js",
      "types": "./dist/types/interface.d.ts"
    },
    "./*": "./*"
  },
  "files": [
    "dist",
    "webpack-plugin.js"
  ],
  "sideEffects": false,
  "repository": {
    "type": "git",
